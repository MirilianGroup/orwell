{
	"ApplicationUrl": "http://orwell:26490",
	"Inquisitor": {
		"Enabled": true
	},
	"Workspace": {
		"Enabled": true
	},
	"RabbitMQ": [
		{
			"Name": "rabbitmq",
			"Exchange": {
				"Declare": true,
				"Name": "logs",
				"Type": "topic",
				"Durable": true,
				"AutoDelete": false,
				"NoWait": false,
				"Arguments": {
					"example": "object"
				}
			},
			"Queue": {
				"Declare": true,
				"Name": "orwell",
				"Durable": true,
				"Exclusive": false,
				"AutoDelete": false,
				"Passive": false,
				"NoWait": false,
				"Arguments": {
					"example": "object"
				}
			},
			"RoutingKey": ""
		}
	],
	"Redis": [
		{
			"Name": "development",
			"StreamName": "orwell-logs",
			"ConsumerGroup": "orwell-logs",
			"BatchSize": 100,
			"BlockTimeoutSeconds": 1,
			"PendingBlockTimeoutSeconds": 5
		}
  	]
}
